<UserControl x:Class="PavigymDeveloperTest.UserControls.UCTitle"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DockPanel>
        <Image Source="../Img/pavigym.png" Width="54" DockPanel.Dock="Right" Margin="5,0,0,0" />
        <TextBlock Text="{Binding Title, RelativeSource={RelativeSource AncestorType=Window}}" Style="{StaticResource titleStyle}" />
    </DockPanel>
</UserControl>
